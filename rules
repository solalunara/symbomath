#DistributeAddition
	m * ( a + b ) -> m * a + m * b
#FactorAddition
	m * a + m * b -> m * ( a + b )
#PreferAddition
	( a + b ) * / c -> a * / c + b * / c
	( a * d + b * c ) * / ( c * d ) ->  a * / c + b * / d
#PreferMultiplication
	a * / c + b * / c -> ( a + b ) * / c
	a * / c + b * / d -> ( a * d + b * c ) * / ( c * d )
#PreferRepeatMult
	exp( a * n ) -> exp( a ) * exp( a ) * ... n times
#PreferExpToMult
	exp( a ) * exp( a ) -> exp( 2 * a )
	exp( n * a ) * exp( a ) -> exp( ( n + 1 ) * a )
#ExpandLnMultToAdd / CondenseLnAddToMult
	ln( a * b ) = ln( a ) + ln( b )
#ExpandLnDivToNeg / CondenseLnNegToDiv
	ln( / x ) = - ln( x )
#ExpandExpAddToMult / CondenseExpMultToAdd
	exp( a + b ) = exp( a ) * exp( b )
#ExpandExpNegToDiv / CondenseExpDivToNeg
	exp( -a ) = / exp( a )